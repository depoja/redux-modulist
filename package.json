{
  "name": "redux-modulist-workspace",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/dev.config.js",
    "build": "webpack --config ./webpack/prod.config.js",
    "build:detailed": "webpack --config ./webpack/analyze.config.js",
    "test": "yarn test:jest && start-server-and-test start http://localhost:3100 test:cy",
    "test:cy": "cypress run",
    "test:jest": "jest",
    "cy": "cypress open"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cypress": "^2.1.0",
    "jest": "^22.4.3",
    "start-server-and-test": "^1.4.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
